  "browser": {
    "stream": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Chrome >= 61",
    "Firefox >= 60",
    "Edge >= 16",
    "Safari >= 11",
    "iOS >= 11"
