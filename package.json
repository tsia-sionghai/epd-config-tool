    "@emotion/styled": "^11.13.0",
    "@hello-pangea/dnd": "^17.0.0",
    "@mui/material": "^6.1.7",
    "axios": "^1.7.7",
    "i18next": "^23.16.2",
    "jszip": "^3.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.10",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.27.0",
    "ts-md5": "^1.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.8.2",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-dropzone": "^4.2.2",
    "@vitejs/plugin-react": "^4.3.2",
